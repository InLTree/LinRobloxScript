{
  "Pets": {
    "<宠物名称>": {
      "Icon": "rbxassetid://<数字ID>",  // 宠物图标资源链接
      "DefaultHunger": <数值>          // 宠物初始饥饿值
    }
  },
  "PetMutationsCode": {
    "<缩写代码>": "<变异名称>"  // 宠物变异的缩写与全称对应（如"a": "Shocked"）
  },
  "Fruits": {
    "<水果名称>": [
      <数值1>,  // 推测为水果重量/体积
      <数值2>,  // 推测为水果价值/点数
      <数值3>   // 推测为水果提供的饥饿恢复量
    ]
  },
  "Mutations": {
    "<变异效果名称>": {
      "Id": "<变异ID>",          // 变异唯一标识
      "ValueMulti": <数值>,      // 变异效果倍率
      "Name": "<变异显示名称>",  // 与ID对应的完整名称（部分与键名一致）
      "Color": {                 // 变异关联颜色（RGB值）
        "r": <0-255>,
        "g": <0-255>,
        "b": <0-255>
      },
      "StrokeColor": {           // 部分变异含描边颜色（可选字段）
        "r": <0-255>,
        "g": <0-255>,
        "b": <0-255>
      }
    }
  },
  "Crates": [
    "<物品名称1>",  //  crate可开出的物品列表（如装饰、道具等）
    "<物品名称2>",
    "<物品名称3>"
  ],
  "PetMutations": [
    "<宠物变异类型1>",  // 宠物可获得的变异类型列表（与PetMutationsCode的全称对应）
    "<宠物变异类型2>",
    "<宠物变异类型3>"
  ]
}
